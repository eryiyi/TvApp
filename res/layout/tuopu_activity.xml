<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:id="@+id/main"
              android:background="@color/mm_main_bg"
              android:layout_width="match_parent"
              android:layout_height="match_parent">
    <RelativeLayout android:layout_width="match_parent"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:background="@color/mm_main_bg_mine_top"
                    android:layout_height="@dimen/top_title_height">
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:id="@+id/back"
                android:onClick="back"
                android:paddingLeft="10dp"
                android:src="@drawable/md_return"
                android:paddingRight="10dp"
                />
        <TextView android:layout_width="wrap_content"
                  android:text="认识他"
                  android:textSize="18sp"
                  android:layout_centerHorizontal="true"
                  android:layout_centerVertical="true"
                  android:textColor="@color/white"
                  android:layout_height="wrap_content"/>
    </RelativeLayout>

    <RelativeLayout
                    android:layout_width="match_parent"
                    android:background="@color/white"
                    android:layout_height="match_parent">

        <FrameLayout
                android:id="@+id/content_frame"
                android:layout_width="match_parent"
                android:background="@color/white"
                android:layout_marginBottom="60dp"
                android:layout_height="match_parent">
        </FrameLayout>

        <LinearLayout android:layout_width="match_parent"
                      android:orientation="horizontal"
                      android:layout_alignParentBottom="true"
                      android:layout_height="60dp">
            <RelativeLayout android:layout_width="0dp"
                            android:layout_weight="1"
                            android:padding="6dp"
                            android:id="@+id/foot_liner_one"
                            android:layout_gravity="center"
                            android:layout_height="wrap_content">
                <TextView android:layout_width="wrap_content"
                          android:id="@+id/foot_one"
                          android:textSize="18sp"
                          android:layout_centerHorizontal="true"
                          android:text="推荐路线"
                          android:textColor="@color/textColortwo"
                          android:gravity="center"
                          android:layout_height="match_parent"/>
            </RelativeLayout>

            <ImageView android:layout_width="1dp"
                       android:layout_gravity="center"
                       android:background="@color/gray01"
                       android:layout_height="40dp"/>
            <RelativeLayout android:layout_width="0dp"
                            android:layout_weight="1"
                            android:padding="6dp"
                            android:layout_gravity="center"
                            android:id="@+id/foot_liner_two"
                            android:layout_height="wrap_content">
                <TextView android:layout_width="wrap_content"
                          android:id="@+id/foot_two"
                          android:textSize="18sp"
                          android:layout_centerHorizontal="true"
                          android:text="任性路线"
                          android:textColor="@color/textColortwo"
                          android:gravity="center"
                          android:layout_height="match_parent"/>
            </RelativeLayout>

            <ImageView android:layout_width="1dp"
                       android:visibility="gone"
                       android:layout_gravity="center"
                       android:background="@color/gray01"
                       android:layout_height="40dp"/>

            <RelativeLayout android:layout_width="0dp"
                            android:layout_weight="1"
                            android:padding="6dp"
                            android:visibility="gone"
                            android:layout_gravity="center"
                            android:id="@+id/foot_liner_three"
                            android:layout_height="wrap_content">
                <TextView android:layout_width="wrap_content"
                          android:id="@+id/foot_three"
                          android:textSize="18sp"
                          android:layout_centerHorizontal="true"
                          android:text="最短线路"
                          android:textColor="@color/textColortwo"
                          android:gravity="center"
                          android:layout_height="match_parent"/>

            </RelativeLayout>
        </LinearLayout>
        <View android:layout_width="match_parent"
              android:visibility="gone"
              android:layout_alignTop="@+id/home_item_photo"
              android:background="@color/gray_line_color"
              android:layout_height="0.5dp"></View>

    </RelativeLayout>


